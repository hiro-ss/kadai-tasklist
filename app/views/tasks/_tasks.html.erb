<% tasks.each do |task| %>

<%= link_to task.user.name, user_path(task.user) %> <span>tasked at <%= task %></span>

<%= task.content %>
<%= task.status %>

  <% if current_user == task.user %>
     <%= link_to "Delete", task, method: :delete, data: { confirm: "You sure?" } %>
  <% end %>

<% end %>
<%= paginate tasks %>